<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.site.chanchanchan.mapper.MemberMapper">
	
	<select id="select" parameterType="String" resultType="Member">
		SELECT * FROM member WHERE member_id=#{member_id}
	</select>
	<select id="selectall" resultType="Member">
		SELECT * FROM member
	</select>
	<insert id="insert" parameterType="Member">
		INSERT INTO member VALUES (#{member_index},#{member_id},#{member_pw},#{member_name},#{member_email},
		#{member_tel},#{member_rank},#{member_status},#{member_joindate},#{member_dropdate})
	</insert>
	<update id="update" parameterType="Member">
		UPDATE member SET #{member_id},#{member_pw},#{member_name},#{member_email},
		#{member_tel},#{member_rank},#{member_status},#{member_joindate},#{member_dropdate}
		WHERE member_id=#{member_id}
	</update>
	<delete id="delete" parameterType="String">
		DELETE FROM member WHERE member_id=#{member_id}
	</delete>
	
</mapper>